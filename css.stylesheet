section.image img
{
  width:100%;

}


div.logo
i
{
margin-left:-120px;
position:absolute;
padding-top: 10px;
color:black;
}

div.menucontainer

{
    
    background-color: whitesmoke;
    width:100%;
    border-top-right-radius: 40px;
    border-top-left-radius: 40px;
    margin-top: -50px;
    position: relative;
          
}


div.menu
a
{
text-decoration: none;
color:black;
}
div.menu
h1
{
    font-size: 25px;
    padding-left: 20px;
    padding-top: 10px;
    margin-bottom: 10px;
    font-family:'shrikhand';
    
}
div.menu
h2
{
    font-size: 20px; 
    padding-top: 10px;
    margin-bottom: 10px;
    padding-left: 10px;
    
}

div.menu
h2:hover
{
    border-bottom:#99E2D0 solid;
    width:10%;   
}

div.menu
i
{
    padding-left: 130px;
}



