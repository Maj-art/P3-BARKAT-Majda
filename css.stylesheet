*
{
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}

body
{
    font-family: 'Roboto', sans-serif;
    font-size: 15px;
    width:100%;
    margin: auto;
    
}

header
div.logo
{
    width:100px;
    padding-left: 30%;
    font-family:'shrikhand';
    
}
header
section.inputheader
{
background-color:#f5f5f5;
padding-left: 30%;

}

div.containerexplorer
input
{
    font-size: 1em;
    border: hidden;
    height: 50px;
    background-color:lightgray;
    text-align: center;   
    
}
section.input
{
    background-color:lightgray;
}

div.containerexplorer
h1
{
    font-size: 2em;
    font-weight: 700;
    margin-bottom: 1em;
    font-family: 'shrikhand';
}

div.containerexplorer
h2
{
    font-size: x-large;
    font-weight: 500;
    margin-bottom: 2em;
    font-family:'shrikhand';
}
div.containerexplorer
a.bouton
{
    display: inline-block;
    border: none;
    border-radius: 30px;
    padding: 8px;
    text-decoration: none;
    color: white;
    background: linear-gradient(#FF79DA, #9356DC);
}
   


div.containerexplorer
{
    background-color: whitesmoke;
    width:100%;
    margin:auto;
    text-align: center;
}

div.containerexplorer
.title
{
    padding: 4em 2em;
    font-family: 'shrikhand';
}

div.containerfonctionnement
{
    width: 100%;
    margin: auto;
    padding: 4em 2em;
}

div.number
{
    position: absolute;
    color: white;
    border-radius: 20px;
    width:20px;
    height:20px;
    background-color: #9356DC;
    top: 15px;
    left: -10px;
}


div.containerfonctionnement
h1
{
    font-size:1.4em;
    margin-bottom:50px;
    font-family: 'shrikhand';
}
div.containerfonctionnement
a
{
    display: block;
    text-decoration: none;
    color: black;
    margin-bottom: 20px;
    border:black transparent;
    border-radius: 10px;
    background-color: whitesmoke;
    width: 100%;
    text-align:center;
    padding:15px;
    box-shadow: 0px 3px 5px 1px #ccc; 
    position: relative;
}

div.containerimages
{
    width: 100%;
    margin:auto;
    padding: 2em;
    background-color: whitesmoke;
}

div.contentimages
{
    background-color: white;
    width: 100%;
    border-radius: 20px;
    height:200px;
    margin-bottom: 20px;
    box-shadow: 0px 3px 5px 1px #ccc;
}

div.containerimages
h1
{
    font-size: 1.4em;
    width:100%;
    margin:auto;
    margin-bottom: 10px;
    font-family:'shrikhand';
}

div.contentimages img
{
    width: 100%;
    object-fit: cover;
    overflow: hidden;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    height:70%;
    position: relative;
}
div.contentimages
a.nouveau
{
    position:absolute;
    text-decoration: none;
    margin-top: -120px;
    margin-left: 280px;
    text-align:right;
    color:#399780;
    font-weight: 100;
    background-color:#99E2D0;
    height:25px;
    width:80px;
    text-align:center;  
}


div.contentimages
h3
{
    font-size: 1em;
    line-height: 0%;
    padding-left: 10px;
    font-family: 'shrikhand';

}

div.contentimages
p
{
    font-size: 1em;
    padding-left: 10px;
    padding-top:5px;
}

div.contentimages
i
{
    padding-left: 200px;
    font-size: 20px;
    transition: color 2s;
    
    
}
i:hover
{
color:  #9356DC;
}

div.containerfooter
{
    background-color: black;
    width:100%;
    margin:auto;
       
}

div.containerfooter 
img

{
    color:white;
    font-size: 1em;

}

div.containerfooter
a
{
    text-decoration: none; 
    color:white;     
}


div.containerfooter
img
{
    color:white;
}

div.containerfooter
i
{
    font-size: 0.7em;
    padding-right: 5px;
}

